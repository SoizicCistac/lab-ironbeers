<img id="beerImg" src={{this.image_url}} alt="beer image">
<div id="info">
    <h3>{{this.name}}</h3>
    <p id="tagline">{{this.tagline}}</p>
    <p id="description">{{this.description}}</p>
    <h4>Food Pairing</h4>
    <ul>
        {{#each this.food_pairing}}
        <li>
            {{this}}
        </li>
        {{/each}}
    </ul>
    <p id="brewer"><span>Brewers Tips: </span>{{this.brewers_tips}}</p>
</div>